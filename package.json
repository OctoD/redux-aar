{
  "name": "redux-aar",
  "version": "1.0.0",
  "licence": "MIT",
  "author": {
    "name": "Paolo Roth",
    "email": "iamoctod@gmail.com"
  },
  "main": "dist/index.js",
  "description": "Actions creator and reducers utilities for redux",
  "keywords": [
    "action creator",
    "action-creator",
    "octod",
    "react-redux",
    "react",
    "redux",
    "state-manager",
    "state",
    "typescript"
  ],
  "scripts": {
    "changelog": "standard-changelog",
    "docs": "typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "tsc -p tsconfig.npm.json",
    "size-limit": "tsc -p . && size-limit",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/yourname/yourrepo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/yourrepo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "limit": "1 s",
      "webpack": false,
      "path": "dist/*.js"
    }
  ],
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "codecov": "^3.5.0",
    "eslint": "^6.4.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "size-limit": "^2.1.5",
    "standard-changelog": "^2.0.13",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.4",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "redux": "^4.0.4"
  }
}
